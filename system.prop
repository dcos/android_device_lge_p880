#
# system.prop for p880
#

ro.sf.lcd_density=320

rild.libpath=/system/lib/lge-ril.so
rild.libargs=-d /dev/ttyACM0
ro.telephony.ril_class=X3RIL

ro.opengles.version=131072

persist.sys.usb.config=mtp,adb

# NFC
debug.nfc.fw_download=true
debug.nfc.se=false
ro.nfc.port=I2C
lge.nfc.fwpatchv13=yes
lge.nfc.indicator=yes
lge.nfc.defaultonoff=off

wifi.interface=wlan0

ro.bt.bdaddr_path=/sys/devices/platform/bd_address/bdaddr_if

# don't preload OpenGL in Zygote, the Tegra drivers do not like it
>>>>>>> 3998e1e... GPU producer to CPU consumer is supported with grouper blobs
ro.zygote.disable_gl_preload=true


# Disable strictmode when compiling -eng
persist.sys.strictmode.disable=1
